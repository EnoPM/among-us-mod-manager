    "build": "webpack --config webpack.build.config.js --mode production",
    "package": "npm run build",
    "postpackage": "electron-packager ./ \"Linkweb Softphone\" --out=./builds --icon=./src/assets/images/electron.icns",
    "dmg": "electron-installer-dmg \"/Users/linkweb/PhpstormProjects/electron-webpack-phone/builds/Linkweb Softphone-darwin-x64/Linkweb Softphone.app\" \"softphone\" --out=\"/Users/linkweb/PhpstormProjects/electron-webpack-phone/builds/Linkweb Softphone-darwin-x64/\"",
    "publish": "npm run clean && npm run build && npm run postpackage && npm run dmg",
    "clean": "rm -rf ./dist ./builds",
    "rebuild": "npm rebuild --runtime=electron --target=9.0.5 --disturl=https://atom.io/download/atom-shell --abi=83"
  },
